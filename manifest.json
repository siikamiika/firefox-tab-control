{
    "manifest_version": 2,
    "name": "Tab Control",
    "version": "0.1",
    "description": "Control tabs from the OS",
    "author": "siikamiika",
    "background": {"page": "bg/background.html"},
    "minimum_chrome_version": "57.0.0.0",
    "options_ui": {
        "page": "bg/settings.html"
    },
    "permissions": [
        "nativeMessaging",
        "tabs",
        "sessions"
    ],
    "applications": {
        "gecko": {
            "id": "tab_control@miika.es",
            "strict_min_version": "52.0"
        }
    }
}
